{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
  "id": "co.raphii.streamdeck-display-brightness.dial-control",
  "items": [
    {
      "key": "icon",
      "type": "pixmap",
      "rect": [8, 8, 32, 32]
    },
    {
      "key": "title",
      "type": "text",
      "rect": [46, 8, 146, 24],
      "alignment": "left",
      "font": {
        "size": 16,
        "weight": 600
      },
      "color": "#FFFFFF"
    },
    {
      "key": "value",
      "type": "text",
      "rect": [46, 32, 146, 30],
      "alignment": "left",
      "font": {
        "size": 24,
        "weight": 700
      },
      "color": "#FFFFFF"
    },
    {
      "key": "indicator",
      "type": "gbar",
      "rect": [8, 74, 184, 18],
      "value": 0,
      "range": {
        "min": 0,
        "max": 100
      }
    }
  ]
}
